{
  "$schema": "./App_Plugins/uSync/config/appsettings-usync-schema.json",
  "uSync": {
    "ForceFips": true,
    "Sets": {
      "Default": {
        "DisabledHandlers": [ "" ],
        "Handlers": {
          "DictionaryHandler": {
            "Group": "Settings",
            "Settings": {
              "CreateOnly": true
            }
          }
        }
      }
    },
    "AutoTemplates": {
      "Enabled": true,
      "Delete": true
    }
  }
}