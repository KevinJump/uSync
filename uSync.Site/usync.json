{
  "$schema": "./App_Plugins/uSync/config/appsettings-usync-schema.json",
  "uSync": {
    "ForceFips": true,
    "Settings": {
      "ExportOnSave": "All",
      "UIEnabledGroups":  "All"
    },
    "Sets": {
      "Default": {
        "DisabledHandlers": [ "" ]
      },
      "Custom": {
        "IsSelectable": true,
        "DisabledHandlers": [
          "MediaHandler",
          "ContentHandler",
          "DictionaryHandler",
          "ContentTemplateHandler",
          "DomainHandler",
          "RelationTypeHandler"
        ]
      },
      "Fred": {
        "IsSelectable": true,
        "DisabledHandlers": [ "" ]
      }
    },
    "AutoTemplates": {
      "Enabled": true,
      "Delete": true
    }
  }
}