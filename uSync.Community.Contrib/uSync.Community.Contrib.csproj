<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>

		<PackageId>uSync.Community.Contrib</PackageId>
		<VersionPrefix>9.0.0</VersionPrefix>
		<Authors>Kevin Jump</Authors>
		<Company>Jumoo</Company>

		<PackageIconUrl></PackageIconUrl>
		<PackageLicenseExpression>MPL-2.0</PackageLicenseExpression>
		<PackageProjectUrl>https://jumoo.co.uk/uSync</PackageProjectUrl>
		<PackageTags>Umbraco</PackageTags>
		<RepositoryUrl>https://github.com/KevinJump/uSync8</RepositoryUrl>

		<Description>
			Contribted value mappers for uSync 9.
		</Description>
		<PackageReleaseNotes>
			9.0 - Dotnet core version
		</PackageReleaseNotes>

		<EmbedUntrackedSources>true</EmbedUntrackedSources>
		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Umbraco.Cms.Web.BackOffice" Version="9.0.0-preview20210706.67279" />
		<PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="All" />
		<PackageReference Include="Umbraco.Cms.Web.Website" Version="9.0.0-preview20210706.67279" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\uSync.Core\uSync.Core.csproj" />
	</ItemGroup>


</Project>
