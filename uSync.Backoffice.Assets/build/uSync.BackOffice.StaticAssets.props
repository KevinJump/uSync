<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup>
		<DefaultItemExcludes>$(DefaultItemExcludes);wwwroot\is-cache\**;wwwroot\ms-cache\**</DefaultItemExcludes>
	</PropertyGroup>

	<PropertyGroup>
		<uSyncInjectFramework>net5.0</uSyncInjectFramework>
		<uSyncInjectTasksFolder>$(MSBuildThisFileDirectory)..\lib\$(uSyncInjectFramework)</uSyncInjectTasksFolder>
		<uSyncInjectSchemaTasksAssembly>$(uSyncInjectTasksFolder)\$(MSBuildThisFileName).dll</uSyncInjectSchemaTasksAssembly>
	</PropertyGroup>

	<UsingTask TaskName="uSyncInjectSchemaTask"
			   AssemblyFile="$(uSyncInjectSchemaTasksAssembly)"/>

	<!--Task parameters default values, this can be overridden-->
	<PropertyGroup>
		<RootFolder Condition="'$(RootFolder)' == ''">$(MSBuildProjectDirectory)</RootFolder>
	</PropertyGroup>

</Project>
