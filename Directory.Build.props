<Project>
    <PropertyGroup>
		<VersionPrefix>9.0.0</VersionPrefix>
		<Authors>Kevin Jump</Authors>
		<Company>Jumoo</Company>

		<PackageIconUrl></PackageIconUrl>
		<PackageLicenseExpression>MPL-2.0</PackageLicenseExpression>
		<PackageProjectUrl>https://jumoo.co.uk/uSync</PackageProjectUrl>
		<PackageTags>Umbraco</PackageTags>

		<RepositoryUrl>https://github.com/KevinJump/uSync8/tree/core/main</RepositoryUrl>

		<Copyright>Jumoo @ 2013-2022</Copyright>

		<PackageIcon>usync-logo.png</PackageIcon>

		<PackageReleaseNotes>
	<![CDATA[9.0 - Dotnet core version
9.1 - Net 5/6 targets
9.2 - Peformance improvements
9.3 - Single pass
9.4 - Fast boot]]>
		</PackageReleaseNotes>

		<EmbedUntrackedSources>true</EmbedUntrackedSources>
		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
    </PropertyGroup>

	<ItemGroup>
		<None Include="../Assets/usync-logo.png" Pack="true" PackagePath="\" />
	</ItemGroup>
</Project>